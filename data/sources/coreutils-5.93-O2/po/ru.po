msgstr "%s: файл слишком велик"

#: src/cksum.c:268
#, c-format
msgid ""
"Usage: %s [FILE]...\n"
"  or:  %s [OPTION]\n"
msgstr ""
"Использование: %s [ФАЙЛ]...\n"
"       или:    %s КЛЮЧ\n"

#: src/cksum.c:273
msgid ""
"Print CRC checksum and byte counts of each FILE.\n"
"\n"
