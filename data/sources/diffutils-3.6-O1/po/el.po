#: src/diff.c:451 src/diff3.c:336
#, c-format
msgid "too many file label options"
msgstr "πάρα πολλές επιλογές ετικέττας αρχείου"

#: src/diff.c:528
#, c-format
msgid "invalid width '%s'"
msgstr "μη έγκυρο πλάτος «%s»"

#: src/diff.c:532
msgid "conflicting width options"
